#include <bits/stdc++.h>
using namespace std;
void findtopo(int node,stack<int>&st,vector<int>&vis,vector<int>adj[])
{
    vis[node]=1;
    for(auto it:adj[node])
    {
        if(vis[it]==0) findtopo(it,st,vis,adj);
    }
    st.push(node);
}
vector<int>toposort(int v,vector<int>adj[])
{
    stack<int>st;
    vector<int>vis(v,0);
    vector<int>topo(v,0);
    for(int i=0;i<v;i++)
    {
        if(vis[i]==0) findtopo(i,st,vis,adj);
    }
    while(!st.empty())
    {
        topo.push_back(st.top());
        st.pop();
    }
    return topo;
}
int main()
{
    int V,E;
    cin>>V>>E;
    vector<int>adj[V];
    for(int i=0;i<E;i++)
    {
        int u,v;
        cin>>u>>v;
        adj[u].push_back(v);
        //adj[v].push_back(u);
    }
   vector<int> res=toposort(V,adj);
   for(int i=V;i<res.size();i++) cout<<res[i]<<" ";

   return 0;
}
